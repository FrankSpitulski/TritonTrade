<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_category_sidebar_page"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.tmnt.tritontrade.categorySidebarPage"
    android:background="@drawable/gradient3">


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true">

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="150dp"
            android:layout_height="match_parent"
            android:layout_alignParentTop="true"
            android:layout_alignParentStart="true"
            android:background="#7aaaaaaa">

            <Button
                android:text="Clothing"
                android:layout_width="match_parent"
                android:id="@+id/clothingButton"
                android:background="@android:color/transparent"
                android:gravity="bottom|left|center_vertical|center_horizontal"
                android:textColor="@android:color/white"
                android:layout_height="30dp" />
            <View android:id="@+id/primerdivisor1"
                android:layout_width="fill_parent"
                android:background="#aaa"
                android:layout_below="@+id/textView6"
                android:layout_alignParentStart="true"
                android:layout_height="2sp" />
            <Button
                android:text="Food"
                android:layout_width="match_parent"
                android:id="@+id/foodButton"
                android:background="@android:color/transparent"
                android:gravity="bottom|left|center_vertical|center_horizontal"
                android:textColor="@android:color/white"
                android:layout_height="30dp" />
            <View android:id="@+id/primerdivisor2"
                android:layout_height="2sp"
                android:layout_width="fill_parent"
                android:background="#aaa"
                android:layout_below="@+id/textView6"
                android:layout_alignParentStart="true"
                 />
            <Button
                android:text="Furniture"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:id="@+id/furnitureButton"
                android:background="@android:color/transparent"
                android:gravity="bottom|left|center_vertical|center_horizontal"
                android:textColor="@android:color/white" />
            <View android:id="@+id/primerdivisor3"
                android:layout_height="2sp"
                android:layout_width="fill_parent"
                android:background="#aaa"
                android:layout_below="@+id/textView6"
                android:layout_alignParentStart="true"
                />
            <Button
                android:text="Services"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:id="@+id/servicesButton"
                android:background="@android:color/transparent"
                android:gravity="bottom|left|center_vertical|center_horizontal"
                android:textColor="@android:color/white" />
            <View android:id="@+id/primerdivisor4"
                android:layout_height="2sp"
                android:layout_width="fill_parent"
                android:background="#aaa"
                android:layout_below="@+id/textView6"
                android:layout_alignParentStart="true"
                 />
            <Button
                android:text="Storage"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:id="@+id/storageButton"
                android:background="@android:color/transparent"
                android:gravity="bottom|left|center_vertical|center_horizontal"
                android:textColor="@android:color/white" />
            <View android:id="@+id/primerdivisor5"
                android:layout_height="2sp"
                android:layout_width="fill_parent"
                android:background="#aaa"
                android:layout_below="@+id/textView6"
                android:layout_alignParentStart="true"
                 />
            <Button
                android:text="Supplies"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:id="@+id/suppliesButton"
                android:background="@android:color/transparent"
                android:gravity="bottom|left|center_vertical|center_horizontal"
                android:textColor="@android:color/white" />
            <View android:id="@+id/primerdivisor6"
                android:layout_height="2sp"
                android:layout_width="fill_parent"
                android:background="#aaa"
                android:layout_below="@+id/textView6"
                android:layout_alignParentStart="true"
                 />
            <Button
                android:text="Technology"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:id="@+id/technologyButton"
                android:background="@android:color/transparent"
                android:gravity="bottom|left|center_vertical|center_horizontal"
                android:textColor="@android:color/white" />
            <View android:id="@+id/primerdivisor7"
                android:layout_height="2sp"
                android:layout_width="fill_parent"
                android:background="#aaa"
                android:layout_below="@+id/textView6"
                android:layout_alignParentStart="true"
                 />
            <Button
                android:text="Textbooks"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:id="@+id/textbooksButton"
                android:background="@android:color/transparent"
                android:gravity="bottom|left|center_vertical|center_horizontal"
                android:textColor="@android:color/white" />
            <View android:id="@+id/primerdivisor8"
                android:layout_height="2sp"
                android:layout_width="fill_parent"
                android:background="#aaa"
                android:layout_below="@+id/textView6"
                android:layout_alignParentStart="true"
                 />
            <Button
                android:text="Misc."
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:id="@+id/miscButton"
                android:background="@android:color/transparent"
                android:gravity="bottom|left|center_vertical|center_horizontal"
                android:textColor="@android:color/white" />
            <View android:id="@+id/primerdivisor9"
                android:layout_height="2sp"
                android:layout_width="fill_parent"
                android:background="#aaa"
                android:layout_below="@+id/textView6"
                android:layout_alignParentStart="true"/>

        </LinearLayout>
    </RelativeLayout>
</RelativeLayout>
